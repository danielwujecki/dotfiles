// Geben Sie Ihre Tastenzuordnungen in dieser Datei ein, um die Standardwerte au√üer Kraft zu setzen.
[
    {
        "key": "ctrl+shift+j",
        "command": "workbench.action.togglePanel"
    },
    {
        "key": "ctrl+shift+c",
        "command": "-workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    },
    {
        "key": "ctrl+shift+c",
        "command": "cmake.compileFile"
    }
]

{
    "telemetry.enableTelemetry": false,
    "editor.minimap.enabled": false,
    "latex-workshop.view.pdf.viewer": "external",
    "files.trimTrailingWhitespace": true,
    "editor.formatOnPaste": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "window.zoomLevel": 1,
    "extensions.ignoreRecommendations": true,
    "python.jediEnabled": true,
    "python.linting.pylintArgs": [
        "--disable=C0111,C0301,C0103",
        // "--extension-pkg-whitelist=cv2",
        "--generated-members=torch.*,cv2.*"
    ],
    "[python]": {},
    "cSpell.language": "de,en,de-DE",
    "latex-workshop.view.pdf.external.viewer.command": "xreader",
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pug",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "editor.wordWrap": "on",
    "editor.fontSize": 15,
    "editor.lineNumbers": "relative",
    "python.terminal.executeInFileDir": true,
    "cSpell.userWords": [
        "aufbewart",
        "imshow",
        "PYTHONPATH"
    ],
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', 'Droid Sans Fallback'",
    "keyboard.dispatch": "keyCode",
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "explorer.confirmDragAndDrop": false,
    "arduino.defaultBaudRate": 9600,
    "arduino.path": "/usr/bin",
    "rust-client.disableRustup": true,
    "cmake.configureOnOpen": true,
    "workbench.colorTheme": "Material Theme Darker High Contrast",
    "vscode-neovim.neovimPath": "/usr/bin/nvim",
    "vscode-neovim.neovimInitPath": "/home/daniel/.config/nvim/init.vim",
    "python.languageServer": "Jedi",
    "gitlens.hovers.currentLine.over": "line",
    "workbench.iconTheme": "material-icon-theme",
    "jupyter.sendSelectionToInteractiveWindow": true,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.enableTelemetry": false,
    "workbench.colorCustomizations": {
        "[Material Theme Darker High Contrast]": {}
    }
}